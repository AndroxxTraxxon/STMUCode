#include <stdio.h>
#include "vector3.h"

int main(int argc, char* args[]){
	vector3 p = mkVector(-2,1,0);
	vector3 q = mkVector(1,4,-1);
	vector3 r = mkVector(3,6,2);

	//vector3 a = diffVector(q, p);
	//vector3 b = diffVector(r, p);
	vector3 c = crossP(q,r);
	double d = dotP(p,c);
	printV(c);
	printf("\n");
	printf("%lf", d);
	//printf("%.3lf", v3Mag(c)/2);
	getchar();
	return 0;
}
