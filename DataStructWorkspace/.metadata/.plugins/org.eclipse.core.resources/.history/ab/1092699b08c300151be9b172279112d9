#include "vector3.h"
#include "math.h"
double v3Mag(vector3 v){
	return sqrt(v.x * v.x + v.y * v.y + v.z * v.z);
}

vector3 cpVector(vector3 v){
	vector3 copy;
	copy.x = v.x;
	copy.y = v.y;
	copy.z = v.z;
	return copy;
}

vector3 v3Dir(vector3 v){
	vector3 cp = cpVector(v);
	return
}
