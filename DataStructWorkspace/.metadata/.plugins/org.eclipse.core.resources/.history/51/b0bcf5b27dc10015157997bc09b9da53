#include "my_string.h"
#include <stdio.h>
int main(int argc, char* args[]){
	string_type h = Create("Hello ", 6);
	string_type w = Create("World!", 6);
	string_type str = Concatenate(h,w);
	Print(str);

	getchar();
	return 0;
}

void quicksort(string_type arr[], int left, int right){
	int i = left, j = right;
	      string_type tmp;
	      string_type pivot = arr[(left + right) / 2];

	      /* partition */
	      while (i <= j) {
	            while (Compare(arr[i], pivot) < 0)
	                  i++;
	            while (Compare(arr[j], pivot) > 0)
	                  j--;
	            if (i <= j) {
	                  tmp = arr[i];
	                  arr[i] = arr[j];
	                  arr[j] = tmp;
	                  i++;
	                  j--;
	            }
	      };

	      /* recursion */
	      if (left < j)
	            quickSort(arr, left, j);
	      if (i < right)
	            quickSort(arr, i, right);
}
