#include <stdio.h>
#include "vector3.h"

int main(int argc, char* args[]){
	vector3 a = mkVector(2,5,1);
	vector3 b = mkVector(-1,1,0);
	vector3 c = crossP(a,b);
	vector3 d = crossP(b,a);
	printV(c);
	printV(d);
	getchar();
	return 0;
}
